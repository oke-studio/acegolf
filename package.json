{
	"name": "acegolf",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"build": "next build",
		"start": "next start",
		"lint": "next lint",
		"prettier": "prettier --write .",
		"prettier:check": "prettier --check src//**/*.{ts,tsx}",
		"prettier:types": "prettier --write src/types//**/*.ts",
		"contentful:export": "source .env.local &&contentful space export --config src/types/export-config.json --management-token $NEXT_PUBLIC_CONTENTFUL_MANAGEMENT_ACCESS_TOKEN --space-id $NEXT_PUBLIC_CONTENTFUL_SPACE_ID",
		"contentful:generate-types": "cf-content-types-generator src/types/export.json --out src/types/contentful",
		"contentful:update": "npm run contentful:export && npm run contentful:generate-types && npm run prettier:types"
	},
	"dependencies": {
		"@contentful/rich-text-react-renderer": "^15.19.0",
		"@contentful/rich-text-types": "^16.3.0",
		"@emotion/react": "^11.11.0",
		"@emotion/styled": "^11.11.0",
		"@mui/icons-material": "^5.14.7",
		"@mui/material": "^5.13.1",
		"@mui/x-date-pickers": "^6.18.6",
		"@next/bundle-analyzer": "^14.0.4",
		"@studio-freight/lenis": "^1.0.33",
		"@svgr/webpack": "^8.0.1",
		"@tag0/use-text-width": "^1.2.0",
		"@tanstack/react-query": "^5.17.15",
		"@tanstack/react-query-devtools": "^5.17.18",
		"@types/node": "20.2.1",
		"@types/react": "18.2.6",
		"@types/react-dom": "18.2.4",
		"cf-content-types-generator": "^2.13.1",
		"contentful": "^10.6.16",
		"dayjs": "^1.11.10",
		"encoding": "^0.1.13",
		"formik": "^2.4.2",
		"framer-motion": "^10.12.17",
		"graphql": "^16.8.1",
		"graphql-request": "^6.1.0",
		"lint": "^0.8.19",
		"next": "^14.1.0",
		"prettier": "^3.2.4",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-iframe": "^1.8.5",
		"react-player": "^2.13.0"
	},
	"devDependencies": {
		"@tanstack/eslint-plugin-query": "^5.17.7",
		"@typescript-eslint/eslint-plugin": "^5.60.1",
		"@typescript-eslint/parser": "^5.60.1",
		"eslint": "^8.43.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-next": "^14.1.0",
		"eslint-config-standard-with-typescript": "^35.0.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-n": "^15.7.0",
		"eslint-plugin-promise": "^6.1.1",
		"eslint-plugin-react": "^7.32.2",
		"file-loader": "^6.2.0",
		"typescript": "^5.1.3"
	}
}
